{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@components": [
        "src/components"
      ],
      "@screens": [
        "src/screens"
      ],
      "@utils": [
        "src/utils"
      ],
      "@constants": [
        "src/constants"
      ],
      "@hooks": [
        "src/hooks"
      ],
      "@assets": [
        "src/assets"
      ],
      "@types": [
        "src/types"
      ],
      "@providers": [
        "src/providers"
      ],
      "@checkers": [
        "src/checkers"
      ]
    }
  },
}